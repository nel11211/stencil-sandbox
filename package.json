{
  "name": "sandbox",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sandbox/sandbox.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test:basic": "stencil test --spec src/components/my-component/tests/1-basic.spec.ts",
    "test:template": "stencil test --spec src/components/my-component/tests/2-template.spec.tsx",
    "test:before-each": "stencil test --spec src/components/my-component/tests/3-before-each.spec.tsx",
    "test:watch": "stencil test --spec --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.21",
    "jest": "26.6.3",
    "jest-cli": "26.6.3"
  }
}